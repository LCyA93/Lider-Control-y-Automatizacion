<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <data>

        <record id="sequence_hr_expense_advance_transfer" model="ir.sequence">
            <field name="name">HR Advance Payment Sequence</field>
            <field name="code">hr.advance.sequence</field>
            <field name="prefix">HRADV/%(range_year)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field eval="False" name="company_id"/>
            <field name="padding">4</field>
        </record>

    </data>
</odoo>
